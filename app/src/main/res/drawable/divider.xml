<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid
                android:width="1px"
                android:color="#2e000000" />
        </shape>
    </item>

    <item
        android:bottom="1px"
        android:top="1px">
        <shape android:shape="rectangle">
            <solid android:color="#ffffff" />
            <padding
                android:bottom="1px"
                android:top="1px" />
        </shape>
    </item>

</layer-list>